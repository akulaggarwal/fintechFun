{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "compat"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "settings": {
    "polyfills": ["fetch"]
  },
  "rules": {
    "semi": [2, "always"],
    "no-unexpected-multiline": 2,
    "compat/compat": 2,
    "comma-dangle": "off",
    "no-debugger": "warn"
  }
}
